local Flux = loadstring(game:HttpGet"https://raw.githubusercontent.com/DaKiwiX/Roblox-UI-Library/main/Flux")()
local Win = Flux:Window("Kiwi X", "Lucky Blocks", Color3.fromRGB(30, 170, 165), Enum.KeyCode.RightControl)



local Tab1 = Win:Tab("Main", "http://www.roblox.com/asset/?id=6023426915")



Tab1:Label("Spawn Lucky Block")
Tab1:Button("Galaxy Blocks", "", function()
x = 0
while x <= 50 do
    x = x+1
    local Event = game:GetService("ReplicatedStorage").SpawnGalaxyBlock
    Event:FireServer()
    wait()
end
end)

Tab1:Button("Rainbow Blocks", "", function()
x = 0
while x <= 50 do
    x = x+1
    local Event = game:GetService("ReplicatedStorage").SpawnRainbowBlock
    Event:FireServer()
end
end)

Tab1:Button("Diamond Blocks", "", function()
x = 0
while x <= 50 do
    x = x+1
    local Event = game:GetService("ReplicatedStorage").SpawnDiamondBlock
    Event:FireServer()
end
end)

Tab1:Button("Super Blocks", "", function()
x = 0
while x <= 50 do
    x = x+1
    local Event = game:GetService("ReplicatedStorage").SpawnSuperBlock
    Event:FireServer()
end
end)

Tab1:Button("Lucky Blocks", "", function()
x = 0
while x <= 50 do
    x = x+1
    local Event = game:GetService("ReplicatedStorage").SpawnLuckyBlock
    Event:FireServer()
end
end)


local Tab2 = Win:Tab("Local", "http://www.roblox.com/asset/?id=6022668888")


Tab2:Slider("WalkSpeed", "Set your walkspeed", 0, 100,16,function(t)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = (t)
end)

Tab2:Slider("JumpPower", "Set your jumppower", 0, 100,50,function(t)
game.Players.LocalPlayer.Character.Humanoid.JumpPower = (t)
end)

local Discord = Win:Tab("Discord", "http://www.roblox.com/asset/?id=6031280882")
Discord:Label("Discord Server")
Discord:Button("Discord Invite Link", "Copy Discord Invite link", function()
Flux:Notification("Copied Discord Invite Link Paste It Into Your Browser", "Alright")
setclipboard("https://discord.gg/YJFhR2wfzA")
end)
Discord:Label("Press RightControl To Hide Or Show Gui")
Discord:Button("Destroy Gui", "Destroy Kiwi X", function()
game.CoreGui["Kiwi X"]:Destroy()
end)
